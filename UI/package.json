  },
  "devDependencies": {
    "@ui5/cli": "^1.9.0",
    "eslint": "^5.16.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-ui5": "^1.0.0",
    "local-web-server": "^3.0.7",
    "rimraf": "^3.0.0"
